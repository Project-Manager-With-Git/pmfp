{
    "name": "task-script",
    "desc": "脚本任务,一般用于做批处理或者数据库迁移这种低频操作",
    "project-language": "Python",
    "project-type": "application",
    "components": {
        "scripts-task_scripts.py.temp": ["-", "scripts"],
        "universal-submodule": ["-", "-"],
        "universal-config": ["-", "config"],
        "universal-__main__.py.temp": ["-", "__main__"],
        "universal-jsonlog.py.temp": ["-", "log"],
        "docker-Dockerfile.temp": [".", "dockerfile"],
        "docker-pip.conf.temp": [".", "pip"],
        "docker_compose-docker_compose.yml.temp": [".", "docker-compose"]
    },
    "requirement": ["jsonschema"],
    "requirement-dev": [
        "coverage",
        "mypy",
        "mypy-extensions",
        "lxml",
        "wheel",
        "pydocstyle",
        "pep8",
        "autopep8"
    ]
}