package main

import (
	"fmt"
	"plugin"
)

func main() {
	// 加载动态库
	module, err := plugin.Open("./plugins/${project_name}.plugin")
	if err != nil {
		fmt.Println("plugin load error")
	} else {
		//在动态库中查找方法
		Sqrt, err := module.Lookup("Sqrt")
		if err != nil {
			fmt.Println("no Sqrt in plugin")
		} else {
			//为函数赋予类型后再调用
			fmt.Println(Sqrt.(func(float64) float64)(2))
		}
	}
}
