from ._base import register_schema


register_schema("user", {
    "title": "user",
    "description": "query user source",
    "type": "object",
    "properties": {
        "name": {
            "description": "user name",
            "type": "string"
        }, "age": {
            "description": "user age",
            "type": "integer"
        }
    },
    "required": ["name", "age"]
})
